<launch>
    <arg name="id" />

	<include file="$(find data_service)/launch/service.launch" />


	<!-- ARDUINO -->
    <!--  * encoders and ultrasound sensors -->
    <node pkg="rosserial_python" type="serial_node.py" name="arduino_node" >
         <param name="port" value="/dev/ttyUSB0" />
    </node>


    <!-- IMU -->
    <node pkg="myahrs_driver" type="myahrs_driver" name="myahrs_driver">
        <param name="port" value="/dev/ttyACM0" />
        <param name="baud_rate" value="115200" />
    </node>

    <!-- EXPERIMENT-->
	  <node pkg="usb_cam" type="usb_cam_node" name="barc_cam" output="screen">
		<param name="pixel_format" value="yuyv"/>
		<param name="sharpness" value="255"/>
		<param name="saturation" value="255"/>
		<param name="framerate" value="30"/>    
		<param name="contrast" value="5"/>    
		<param name="video_device" value="/dev/video6"/>
	  </node>
	  <node pkg="barc" type="blue_image_processing.py" name="image_processing" output="screen">
		<param name="display_image" value="False" type="boolean"/>
		<param name="publish_image" value="False" type="boolean"/>
		<param name="calibrate_transform" value="False" type="boolean"/>
		<param name="upperLeftX" value="321" type="int"/>
		<param name="upperLeftY" value="141" type="int"/>
		<param name="upperRightX" value="326" type="int"/>
		<param name="upperRightY" value="350" type="int"/>
		<param name="lowerRightX" value="162" type="int"/>
		<param name="lowerRightY" value="433" type="int"/>
		<param name="lowerLeftX" value="148" type="int"/>
		<param name="lowerLeftY" value="65" type="int"/>
	  </node>
    <node pkg="project_231" type="state.py" name="car_state" output="screen">
	</node>


	<!-- RECORD -->
    <node pkg="project_231" type="mpc_record.py" name="record_experiment" output="screen">
        <param name="experiment_name" value="$(arg id)" /> 
        <param name="camera_on" value="True" />
    </node>

	<node pkg="project_231" type="MPC_Tracking.jl" name="mpc_solver" output="screen">
    </node>


    

</launch>
